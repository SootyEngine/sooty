[application]

config/name="sooty_engine"
run/main_scene="res://story_scenes/main_menu.tscn"
config/icon="res://icon.png"
config/features=PackedStringArray("4.0", "Vulkan Clustered")

[autoload]

Sooty="*res://addons/sooty_engine/autoloads/Sooty.gd"
Notify="*res://addons/sys_notifications/autoloads/Notify.gd"
Global="*res://addons/sooty_engine/autoloads/Global.gd"
ModManager="*res://addons/sys_state/autoloads/Mods.gd"
Dialogues="*res://addons/sooty_engine/autoloads/Dialogues.gd"
