Mods="*res://addons/sys_state/autoloads/Mods.gd"
Saver="*res://addons/sys_state/autoloads/Saver.gd"

[debug]

gdscript/warnings/integer_division=false

[editor_plugins]

enabled=PackedStringArray("res://addons/common_file_highlighters/plugin.cfg", "res://addons/simple_vn/plugin.cfg", "res://addons/sooty_engine/plugin.cfg", "res://addons/sys_notifications/plugin.cfg", "res://addons/sys_state/plugin.cfg", "res://addons/tool_button/plugin.cfg")

